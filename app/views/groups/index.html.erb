<div class='content-row'>
  <div class="group-names">
  <p class="group-name">コミュニティー名</p>
    <% @groups.each do |group| %>
      <%= link_to group_path(group.id) do %>
        <p><%= group.group_name %></p>
      <% end %>
    <% end %>
  </div>
  <div class="group-create">
    <%= link_to 'コミュニティー作成', new_group_path %>
  </div>
</div>